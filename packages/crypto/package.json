{
  "name": "@meproto/crypto",
  "version": "0.0.2",
  "description": "Cryptographic operative functions for the Me Protocol.",
  "license": "Apache-2.0",
  "private": false,
  "homepage": "https://meprotocol.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/meproto/ts.git",
    "directory": "packages/crypto"
  },
  "bugs": {
    "url": "https://github.com/meproto/ts/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup src/index.ts --dts --format esm --target es2022 --no-splitting",
    "test": "vitest"
  },
  "tsup": {
  "entry": ["src/index.ts"],
  "format": ["esm"],
  "dts": true,
  "splitting": false,
  "sourcemap": true,
  "clean": true
},
  "keywords": [
    "did",
    "didme",
    "meproto",
    "decentralized-identity",
    "identity",
    "pqc",
    "post-quantum",
    "pqc-signatures",
    "mldsa87",
    "mlkem1024",
    "ed25519",
    "x25519",
    "es256",
    "w3c",
    "w3c-did",
    "did-core",
    "eu-wallet",
    "eudi-wallet",
    "verifiable-credentials",
    "digital-identity",
    "cryptography"
  ],
  "sideEffects": true,
  "dependencies": {
    "@meproto/cbor": "^0.0.1",
    "@meproto/codec": "^0.0.3",
    "@meproto/crypto-primitives": "^0.0.2",
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^2.0.1",
    "@noble/post-quantum": "^0.5.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0"
  }
}