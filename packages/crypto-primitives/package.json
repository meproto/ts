{
  "name": "@meproto/crypto-primitives",
  "version": "0.0.2",
  "description": "Cryptographic primitives for the Me Protocol.",
  "license": "Apache-2.0",
  "private": false,
  "homepage": "https://meprotocol.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/meproto/ts.git",
    "directory": "packages/crypto-primitives"
  },
  "bugs": {
    "url": "https://github.com/meproto/ts/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "tsup": {
