    "ed25519",
    "x25519",
    "es256",
    "w3c",
    "w3c-did",
    "did-core",
    "eu-wallet",
    "eudi-wallet",
    "verifiable-credentials",
    "digital-identity",
    "cryptography"
  ],
  "sideEffects": false,
  "dependencies": {
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^2.0.1",
    "@noble/post-quantum": "^0.5.2"
